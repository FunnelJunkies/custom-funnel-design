<script>
$(function tDate (){
	var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
	var date = new Date();
	$("#headline-38676 .elHeadline b:first" ).html(months[date.getMonth()] + ' ' + date.getDate());
});
</script>
